include templates/lang.pug
+var

+head

body
    +header('lang')

    +main([0,9,3])
        - var langs = [{"lang":"c","name":"C","categories":[{"id":0,"title":"基本","list":[{"filename":"c/main","title":"Main関数"}]}]}]
        div.row.mx-auto
            each l in langs
                +lang-card(l.lang, l.name, l.categories)

    +footer([{
        filename: 'index.html',
        title: 'Top'
    }, {
        current: true,
        filename: 'lang.html',
        title: 'Lang'
    }])
